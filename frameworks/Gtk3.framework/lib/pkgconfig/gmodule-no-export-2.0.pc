prefix=/Library/Frameworks/Gtk3.framework
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

gmodule_supported=true

Name: GModule
Description: Dynamic module loader for GLib
Requires: glib-2.0
Version: 2.34.3
Libs: -L${libdir} -lgmodule-2.0 
Libs.private: 
Cflags: -D_REENTRANT
